{
  "name": "server",
  "version": "1.0.0",
  "description": "Booking hotel platform",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "seed": "node database/seeders/seeder.js",
    "seed:user": "cross-env NODE_ENV=development node database/seeders/user.seed.js",
    "seed:review": "cross-env NODE_ENV=development node database/seeders/review.seed.js",
    "seed:room": "cross-env NODE_ENV=development node database/seeders/room.seed.js",
    "seed:booking": "cross-env NODE_ENV=development node database/seeders/booking.seed.js",
    "seed:nearby_place": "cross-env NODE_ENV=development node database/seeders/nearby_place.seed.js",
    "seed:notification": "cross-env NODE_ENV=development node database/seeders/notification.seed.js",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migrate:create": "sequelize-cli migration:generate --name",
    "migrate:seed": "sequelize-cli db:seed:all",
    "migrate:seed:undo": "sequelize-cli db:seed:undo:all",
    "start:consumer": "cross-env NODE_ENV=development node kafka/worker.js"
  },
  "_moduleAliases": {
    "@controllers": "controllers",
    "@validators": "validators",
    "@routes": "routes",
    "@config": "config",
    "@database": "database",
    "@email-templates": "email-templates",
    "@middlewares": "middlewares",
    "@models": "models",
    "@public": "public",
    "@repositories": "repositories",
    "@services": "services",
    "@kafka": "kafka",
    "@socket": "socket",
    "@utils": "utils"
  },
  "author": "Le Duc Phuong",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^9.2.1",
    "@infobip-api/sdk": "^0.3.2",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.0",
    "fs": "^0.0.1-security",
    "joi": "^18.0.2",
    "kafkajs": "^2.2.4",
    "minio": "^8.0.6",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.0",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pino": "^10.1.1",
    "pino-pretty": "^13.1.3",
    "redis": "^4.7.0",
    "sequelize": "^6.37.7",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "stripe": "^17.3.1",
    "uuid": "^10.0.0",
    "uuidv7": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@faker-js/faker": "^10.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-boundaries": "^5.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.5.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.7.4",
    "sequelize-cli": "^6.6.5"
  }
}
